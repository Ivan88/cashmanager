<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.oivan.cashman.model.MonthSchedule" table="MonthSchedule">
		<id name="monthScheduleId" column="monthScheduleId">
			<generator class="assigned"/>
		</id>
		<property name="monthName" column="monthName"/>
		<property name="userId" column="userId"/>
		<property name="expenseItemId" column="expenseItemId"/>
		<property name="amount" column="amount"/>
	</class>
</hibernate-mapping>