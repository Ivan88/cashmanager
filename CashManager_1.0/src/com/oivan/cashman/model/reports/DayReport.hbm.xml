<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.oivan.cashman.model.reports.DayReport" table="DayReport">
		<id name="dayReportId" column="dayReportId">
			<generator class="assigned"/>
		</id>
		<property name="userId" column="userId"/>
		<property name="expensesHistoryId" column="expensesHistoryID"/>
	</class>
</hibernate-mapping>